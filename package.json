		"light",
		"lite",
		"scheme",
		"theme",
		"vscode",
		"darkmode"
	],
	"name": "vscode.huebris-theme",
	"repository": "github:darcher-/vscode.huebris-theme",
	"version": "echo $npm_package_version",
	"scripts": {
		"vsix": "vsce package",
		"test": "jest --coverage"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"ajv": "^8.17.1",
		"jest": "^29.7.0",
