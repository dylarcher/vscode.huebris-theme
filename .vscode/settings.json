{
	"[html]": {
		"editor.codeActionsOnSave": { "source.fixAll": "explicit" },
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnPaste": false,
		"editor.formatOnSave": true,
		"editor.formatOnType": false,
		"prettier.arrowParens": "always",
		"prettier.bracketSameLine": true,
		"prettier.bracketSpacing": true,
		"prettier.embeddedLanguageFormatting": "auto",
		"prettier.endOfLine": "lf",
		"prettier.htmlWhitespaceSensitivity": "css",
		"prettier.jsxSingleQuote": true,
		"prettier.printWidth": 80,
		"prettier.quoteProps": "consistent",
		"prettier.semi": true,
		"prettier.singleAttributePerLine": false,
		"prettier.singleQuote": false,
		"prettier.tabWidth": 2,
		"prettier.trailingComma": "none",
		"prettier.useTabs": true,
		"prettier.vueIndentScriptAndStyle": true
	},
	"[javascript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll"         : "explicit",
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnPaste": false,
		"editor.formatOnSave": true,
		"editor.formatOnType": false,
		"javascript.autoClosingTags": true,
		"javascript.preferences.importModuleSpecifier": "shortest",
		"javascript.preferences.importModuleSpecifierEnding": "js",
		"javascript.preferences.jsxAttributeCompletionStyle": "auto",
		"javascript.preferences.quoteStyle": "double",
		"javascript.preferences.renameMatchingJsxTags": true,
		"javascript.preferences.useAliasesForRenames": true,
		"javascript.suggest.completeJSDocs": true,
		"javascript.suggest.enabled": true,
		"javascript.suggest.jsdoc.generateReturns": true
	},
	"[jsonc]": {
		"editor.codeActionsOnSave": { "source.fixAll": "explicit" },
		"editor.defaultFormatter": "j-brooke.fracturedjsonvsc",
		"editor.formatOnPaste": false,
		"editor.formatOnSave": true,
		"editor.formatOnType": false
	},
	"[mdx]": {
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.codeActionsOnSave": {
			"source.fixAll"         : "explicit",
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "yzhang.markdown-all-in-one",
		"editor.formatOnPaste": false,
		"editor.formatOnSave": true,
		"editor.formatOnType": false
	},
	"[postcss]": {
		"editor.codeActionsOnSave": {
			"source.fixAll"         : "explicit",
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "stylelint.vscode-stylelint",
		"editor.formatOnPaste": false,
		"editor.formatOnSave": true,
		"editor.formatOnType": false
	},
	"[svg]": { "editor.defaultFormatter": "jock.svg" },
	"[xml]": { "editor.defaultFormatter": "redhat.vscode-xml" },
	"[yaml]": {
		"editor.codeActionsOnSave": {
			"source.fixAll"         : "explicit",
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"diffEditor.ignoreTrimWhitespace": true,
	"editor.codeActionsOnSave": {
		"source.fixAll"         : "never",
		"source.organizeImports": "never"
	},
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": false,
	"editor.quickSuggestions": {
		"comments": "off",
		"other"   : "off",
		"strings" : "off"
	},
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"editor.wordWrap": "on",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.expand": false,
	"explorer.fileNesting.patterns": { "*": "${capture}.map, ${basename}.*" },
	"files.associations": {
		"*.{babelrc,svgorc,json}" : "jsonc"          ,
		"*.{cjs,js,jsx,mjs,ts}"   : "javascriptreact",
		"*.{css,sass,scss}"       : "css"            ,
		"*.{editorconfig}"        : "ini"            ,
		"*.{git*,npmrc,nvmrc,env}": "properties"     ,
		"*.{prettierrc,mdlrc}"    : "yaml"
	},
	"workbench.activityBar.iconClickBehavior": "focus",
	"fracturedjsonvsc.v3.AllowTrailingCommas": false,
	"fracturedjsonvsc.v3.AlwaysExpandDepth": -1,
	"fracturedjsonvsc.v3.ColonPadding": true,
	"fracturedjsonvsc.v3.CommaPadding": true,
	"fracturedjsonvsc.v3.CommentPadding": true,
	"fracturedjsonvsc.v3.CommentPolicyForJSON": "Remove",
	"fracturedjsonvsc.v3.CommentPolicyForJSONC": "Preserve",
	"fracturedjsonvsc.v3.MaxCompactArrayComplexity": 4,
	"fracturedjsonvsc.v3.MaxInlineComplexity": 3,
	"fracturedjsonvsc.v3.MaxInlineLength": 80,
	"fracturedjsonvsc.v3.MaxTableRowComplexity": 4,
	"fracturedjsonvsc.v3.MaxTotalLineLength": 80,
	"fracturedjsonvsc.v3.MinCompactArrayRowItems": 4,
	"fracturedjsonvsc.v3.NestedBracketPadding": true,
	"fracturedjsonvsc.v3.OmitTrailingWhitespace": true,
	"fracturedjsonvsc.v3.PreserveBlankLines": true,
	"fracturedjsonvsc.v3.SimpleBracketPadding": true,
	"fracturedjsonvsc.v3.StringWidthPolicy": "CharacterCount",
	"fracturedjsonvsc.v4.NumberListAlignment": "Normalize",
	"js/ts.implicitProjectConfig.checkJs": true,
	"js/ts.implicitProjectConfig.experimentalDecorators": true,
	"js/ts.implicitProjectConfig.module": "ESNext",
	"js/ts.implicitProjectConfig.strictFunctionTypes": true,
	"js/ts.implicitProjectConfig.strictNullChecks": true,
	"js/ts.implicitProjectConfig.target": "ES2022",
	"prettier.enable": false,
	"prettier.requireConfig": false,
	"prettier.resolveGlobalModules": true,
	"prettier.useEditorConfig": true,
	"sortJSON.contextMenu": {
		"sortJSON"                : false,
		"sortJSONAlphaNum"        : true ,
		"sortJSONAlphaNumReverse" : false,
		"sortJSONKeyLength"       : false,
		"sortJSONKeyLengthReverse": false,
		"sortJSONReverse"         : false,
		"sortJSONType"            : false,
		"sortJSONTypeReverse"     : false,
		"sortJSONValues"          : true ,
		"sortJSONValuesReverse"   : false
	},
	"stylelint.config": { "extends": "stylelint-config-standard" },
	"stylelint.ignoreDisables": false,
	"stylelint.packageManager": "npm",
	"stylelint.reportDescriptionlessDisables": true,
	"stylelint.reportInvalidScopeDisables": true,
	"stylelint.reportNeedlessDisables": true,
	"stylelint.validate": [ "css", "postcss", "scss" ],
	"svg.preview.background": "editor",
	"symbols.hidesExplorerArrows": false,
	"workbench.preferredDarkColorTheme": "Visual Studio Dark",
	"workbench.preferredLightColorTheme": "Visual Studio Light",
	"workbench.preferredHighContrastColorTheme": "Default High Contrast",
	"workbench.preferredHighContrastLightColorTheme": "Default High Contrast Light",
	"workbench.productIconTheme": "material-product-icons",
	"workbench.iconTheme": "catppuccin-latte",
	"cody.useContext": "blended",
	"cody.autocomplete.advanced.accessToken": "sgp_a0d7ccb4f752ea73_4077605c5b6cd54d9072ff004606a2931f09c813",
	"cody.autocomplete.advanced.model": "starcoder-16b",
	"cody.autocomplete.advanced.provider": "fireworks",
	"cody.autocomplete.advanced.serverEndpoint": "https://sourcegraph.com/.api/graphql",
	"cody.autocomplete.completeSuggestWidgetSelection": true,
	"cody.autocomplete.enabled": true,
	"cody.commandCodeLenses": true,
	"cody.codeActions.enabled": true,
	"cody.codebase": "ssh://git@github.com/sourcegraph/cody",
	"cody.autocomplete.languages": {
		"*": true,
		"plaintext": false
	},
	"cody.chat.preInstruction": "Answer all my questions using vanilla, native to the browser, Javascript and JSDoc typing instead of typescript.",
	"cody.autocomplete.disableInsideComments": false,
	"cody.autocomplete.formatOnAccept": true,
	"cody.commandHints.enabled": true,
	"cody.edit.preInstruction": "Write all unit tests with the most minimal testing framework available to javscript.",
