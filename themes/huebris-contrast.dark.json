{
	"$schema": "color-theme.schema.json",
	"version": "0.1.0",
	"name": "Huebris Contrast Dark",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#FFFFFF",
		"editor.selectionBackground": "#007ACC",
		"editor.selectionForeground": "#FFFFFF",
		"editor.lineHighlightBackground": "#2A2A2A",
		"editorCursor.foreground": "#FFFFFF",
		"editorLineNumber.foreground": "#FFFFFF",
		"editorWhitespace.foreground": "#707070",
		"editorGutter.background": "#000000",
		"editorGutter.addedBackground": "#32B47A",
		"editorGutter.deletedBackground": "#FF5370",
		"editorGutter.modifiedBackground": "#6796E6",
		"activityBar.background": "#000000",
		"activityBar.foreground": "#FFFFFF",
		"activityBar.border": "#A0A0A0",
		"activityBarBadge.background": "#FFFFFF",
		"activityBarBadge.foreground": "#000000",
		"sideBar.background": "#000000",
		"sideBar.foreground": "#FFFFFF",
		"sideBar.border": "#A0A0A0",
		"sideBarSectionHeader.background": "#0A0A0A",
		"sideBarSectionHeader.foreground": "#FFFFFF",
		"panel.background": "#000000",
		"panel.border": "#A0A0A0",
		"panelTitle.activeBorder": "#FFFFFF",
		"panelTitle.activeForeground": "#FFFFFF",
		"panelTitle.inactiveForeground": "#A0A0A0",
		"statusBar.background": "#000000",
		"statusBar.foreground": "#FFFFFF",
		"statusBar.debuggingBackground": "#FF5370",
		"statusBar.debuggingForeground": "#000000",
		"statusBar.noFolderBackground": "#000000",
		"statusBar.noFolderForeground": "#FFFFFF",
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#FFFFFF",
		"titleBar.inactiveBackground": "#000000",
		"titleBar.inactiveForeground": "#A0A0A0",
		"editorGroupHeader.tabsBackground": "#000000",
		"editorGroup.border": "#A0A0A0",
		"tab.activeBackground": "#000000",
		"tab.activeForeground": "#FFFFFF",
		"tab.activeBorder": "#FFFFFF",
		"tab.inactiveBackground": "#1A1A1A",
		"tab.inactiveForeground": "#A0A0A0",
		"tab.border": "#A0A0A0",
		"focusBorder": "#FFFFFF",
		"input.background": "#0A0A0A",
		"input.foreground": "#FFFFFF",
		"input.border": "#A0A0A0",
		"inputOption.activeBorder": "#FFFFFF",
		"inputValidation.errorBorder": "#FF5370",
		"inputValidation.errorBackground": "#5E0000",
		"inputValidation.errorForeground": "#FFFFFF",
		"inputValidation.infoBorder": "#6796E6",
		"inputValidation.infoBackground": "#00305E",
		"inputValidation.infoForeground": "#FFFFFF",
		"inputValidation.warningBorder": "#FFB74D",
		"inputValidation.warningBackground": "#5E3800",
		"inputValidation.warningForeground": "#FFFFFF",
		"dropdown.background": "#0A0A0A",
		"dropdown.foreground": "#FFFFFF",
		"dropdown.border": "#A0A0A0",
		"list.activeSelectionBackground": "#007ACC",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.focusBackground": "#005288",
		"list.focusForeground": "#FFFFFF",
		"list.hoverBackground": "#00365C",
		"list.hoverForeground": "#FFFFFF",
		"list.inactiveSelectionBackground": "#1A1A1A",
		"list.inactiveSelectionForeground": "#A0A0A0",
		"list.highlightForeground": "#82AAFF",
		"pickerGroup.border": "#A0A0A0",
		"pickerGroup.foreground": "#FFFFFF",
		"button.background": "#A0A0A0",
		"button.foreground": "#000000",
		"button.border": "#A0A0A0",
		"badge.background": "#FFFFFF",
		"badge.foreground": "#000000",
		"progressBar.background": "#FFFFFF",
		"peekView.border": "#A0A0A0",
		"peekViewEditor.background": "#0A0A0A",
		"peekViewEditor.matchHighlightBackground": "#FFFF0080",
		"peekViewResult.background": "#000000",
		"peekViewResult.matchHighlightBackground": "#FFFF00",
		"peekViewTitle.background": "#0A0A0A",
		"peekViewTitleDescription.foreground": "#A0A0A0",
		"peekViewTitleLabel.foreground": "#FFFFFF",
		"editor.findMatchBackground": "#FFFF00",
		"editor.findMatchHighlightBackground": "#FFFF0080",
		"errorForeground": "#FF5370",
		"selection.background": "#007ACC"
	},
	"tokenColors": [
		{
			"scope": "variable.parameter.function",
			"settings": { "foreground": "#82AAFF" }
		},
		{
			"scope": "comment, punctuation.definition.comment",
			"settings": { "foreground": "#909090" }
		},
		{
			"scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array",
			"settings": { "foreground": "#FFFFFF" }
		},
		{
			"scope": "none",
			"settings": { "foreground": "#FFFFFF" }
		},
		{
			"scope": "keyword.operator",
			"settings": { "foreground": "#FF89A1" }
		},
		{
			"scope": "entity.name.atomtag.vinala-atomium",
			"settings": { "foreground": "#FF89A1" }
		},
		{
			"scope": "entity.name.echo.vinala-atomium",
			"settings": { "foreground": "#C792EA" }
		},
		{
			"scope": "keyword",
			"settings": { "foreground": "#7FFFD4" }
		},
		{
			"scope": "variable",
			"settings": { "foreground": "#C792EA" }
		},
		{
			"scope": "entity.name.function, meta.require, support.function.any-method",
			"settings": { "foreground": "#A6E22E" }
		},
		{
			"scope": "support.class, entity.name.class, entity.name.type.class",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "meta.class",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "keyword.other.special-method",
			"settings": { "foreground": "#82AAFF" }
		},
		{
			"scope": "storage",
			"settings": { "foreground": "#82AAFF" }
		},
		{
			"scope": "support.function",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "string, constant.other.symbol, entity.other.inherited-class",
			"settings": { "foreground": "#FF89A1" }
		},
		{
			"scope": "constant.numeric",
			"settings": { "foreground": "#FFC78A" }
		},
		{
			"scope": "constant",
			"settings": { "foreground": "#FFC78A" }
		},
		{
			"scope": "entity.name.tag",
			"settings": { "foreground": "#80CBC4" }
		},
		{
			"scope": "entity.name.subtag",
			"settings": { "foreground": "#A6E22E" }
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "entity.other.attribute-name.id, punctuation.definition.entity",
			"settings": { "foreground": "#82AAFF" }
		},
		{
			"scope": "meta.selector",
			"settings": { "foreground": "#C792EA" }
		},
		{
			"scope": "markup.heading punctuation.definition.heading, entity.name.section",
			"settings": { "foreground": "#82AAFF", "fontStyle": "" }
		},
		{
			"scope": "keyword.other.unit",
			"settings": { "foreground": "#FFC78A" }
		},
		{
			"scope": "markup.bold, punctuation.definition.bold",
			"settings": { "foreground": "#FFC78A", "fontStyle": "bold" }
		},
		{
			"scope": "markup.italic, punctuation.definition.italic",
			"settings": { "foreground": "#C792EA", "fontStyle": "italic" }
		},
		{
			"scope": "markup.raw.inline",
			"settings": { "foreground": "#A6E22E" }
		},
		{
			"scope": "string.other.link",
			"settings": { "foreground": "#FF89A1" }
		},
		{
			"scope": "meta.link",
			"settings": { "foreground": "#FFC78A" }
		},
		{
			"scope": "markup.list",
			"settings": { "foreground": "#FF89A1" }
		},
		{
			"scope": "markup.quote",
			"settings": { "foreground": "#FFC78A" }
		},
		{
			"scope": "meta.separator",
			"settings": { "foreground": "#FFFFFF", "background": "#3A3A3A" }
		},
		{
			"scope": "markup.inserted, markup.inserted.git_gutter",
			"settings": { "foreground": "#A6E22E" }
		},
		{
			"scope": "markup.deleted, markup.deleted.git_gutter",
			"settings": { "foreground": "#FF89A1" }
		},
		{
			"scope": "markup.changed, markup.changed.git_gutter",
			"settings": { "foreground": "#82AAFF" }
		},
		{
			"scope": "markup.ignored, markup.ignored.git_gutter",
			"settings": { "foreground": "#707070" }
		},
		{
			"scope": "markup.untracked, markup.untracked.git_gutter",
			"settings": { "foreground": "#707070" }
		},
		{
			"scope": "constant.other.color",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "string.regexp",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "constant.character.escape",
			"settings": { "foreground": "#89DDFF" }
		},
		{
			"scope": "punctuation.section.embedded, variable.interpolation",
			"settings": { "foreground": "#FFC78A" }
		},
		{
			"scope": "invalid.illegal",
			"settings": { "foreground": "#000000", "background": "#FF5370" }
		},
		{
			"scope": "meta.selector, meta.selector entity, meta.selector entity punctuation, entity.name.tag.css",
			"settings": { "foreground": "#82AAFF" }
		},
		{
			"scope": "meta.property-name, support.type.property-name",
			"settings": { "foreground": "#A6E22E" }
		},
		{
			"scope": "keyword.other.important",
			"settings": { "foreground": "#FF5370", "fontStyle": "bold" }
		},
		{
			"scope": "token.info-token",
			"settings": { "foreground": "#6796E6" }
		},
		{
			"scope": "token.warn-token",
			"settings": { "foreground": "#FFB74D" }
		},
		{
			"scope": "token.error-token",
			"settings": { "foreground": "#FF5370" }
		},
		{
			"scope": "token.debug-token",
			"settings": { "foreground": "#C792EA" }
		}
	]
}
